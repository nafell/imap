{
  "name": "imap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && next export",
    "eslint:fix": "eslint ./src --ext .js,jsx,.ts,.tsx --fix",
    "stylelint:fix": "stylelint ./src/**/*.{css,scss} --fix",
    "prettier:fix": "prettier --check --write './src/**/*.{js,jsx,ts,tsx,css,scss,md,mdx}'",
    "format": "npm run eslint:fix && npm run stylelint:fix && npm run prettier:fix",
    "db:gen": "prisma generate",
    "db:mig:dev": "dotenv -e .env.local prisma migrate dev",
    "db:studio": "dotenv -e .env.local prisma studio",
    "db:seed": "dotenv -e .env.local prisma db seed"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.1",
    "@lottiefiles/react-lottie-player": "^3.5.3",
    "@mui/material": "^5.14.11",
    "@prisma/client": "^5.4.2",
    "@supabase/auth-helpers-nextjs": "^0.8.1",
    "@supabase/supabase-js": "^2.38.0",
    "@types/node": "20.7.1",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.50.0",
    "framer-motion": "^10.16.4",
    "next": "^13.5.4",
    "postcss": "^8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.46.2",
    "tailwindcss": "^3.3.3",
    "typescript": "5.2.2",
    "zod": "^3.22.4",
    "zod-prisma-types": "^2.7.9"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "csvtojson": "^2.0.10",
    "encoding": "^0.1.13",
    "eslint-config-next": "^13.5.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "nanoid": "^5.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "prisma": "^5.4.2",
    "sass": "^1.68.0",
    "stylelint": "^15.10.3",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-scss": "^5.2.1",
    "tsx": "^3.13.0"
  }
}
